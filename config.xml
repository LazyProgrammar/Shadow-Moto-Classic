<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Shadow Moto Classic</name>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Pragnesh Patil
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform>
        <preference name="Windows-Target-Version" value="10.0.16299.0" />
        <preference name="Windows.Universal-MinVersion" value="10.0" />
        <preference name="Windows.Universal-MaxVersionTested" value="10.0.16299.0" />
    </platform>    
</widget>
